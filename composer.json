{
    "name": "mcdev/laravel-app-namespace",
    "description": "Provides forward support for app:name",
    "keywords": [
        "laravel",
        "dev",
        "console",
        "namespace",
        "support",
        "library",
        "illuminate"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://collab.devteam.mankatoclinic.com/",
        "source": "https://github.com/MC-RhettW/laravel-app-namespace"
    },
    "authors": [
        {
            "name": "Rhett Waldock",
            "email": "rhettw@mankatoclinic.com",
            "role": "Developer (Fork)"
        },
        {
            "name": "Andrey Helldar",
            "role": "Developer (Upstream)"
        },
        {
            "name": "Taylor Otwell",
            "role": "Developer (Upstream)"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MCDev\\AppNamespaceSupport": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MCDev\\AppNamespaceSupport\\ServiceProvider"
            ]
        }
    }
}
